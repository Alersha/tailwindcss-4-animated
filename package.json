{
    "name": "tailwindcss-animated",
    "version": "1.0.0",
    "description": "Extended animation utilities for Tailwind CSS",
    "author": "new-data-services",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/new-data-services/tailwindcss-animated.git"
    },
    "homepage": "https://tailwindcss-animated.com",
    "keywords": [
        "tailwindcss",
        "tailwind",
        "plugin",
        "animation",
        "animated",
        "keyframes"
    ],
    "files": [
        "/src"
    ],
    "main": "src/index.js",
    "scripts": {
        "dev": "vite --config ./website/vite.config.js",
        "watch": "npm run dev",
        "prod": "vite build --config ./website/vite.config.js",
        "build": "npm run prod",
        "preview": "vite preview --config ./website/vite.config.js",
        "test": "jest",
        "eslint": "npx eslint src/**",
        "eslint:fix": "npx eslint src/** --fix"
    },
    "peerDependencies": {
        "tailwindcss": ">=3.1.0"
    },
    "devDependencies": {
        "@alpinejs/collapse": "^3.10.3",
        "@alpinejs/intersect": "^3.10.3",
        "alpinejs": "^3.10.3",
        "autoprefixer": "^10.4.8",
        "eslint": "^8.40.0",
        "jest": "^29.3.1",
        "postcss": "^8.4.16",
        "postcss-discard-comments": "^5.1.2",
        "postcss-import": "^14.1.0",
        "prettier": "^2.8.3",
        "tailwindcss": "^3.1.0",
        "vite": "^3.0.4",
        "vite-plugin-nunjucks": "^0.1.10"
    },
    "dependencies": {},
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/jest/customMatchers.js"
        ]
    }
}
